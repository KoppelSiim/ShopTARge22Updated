<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - ShopTARge22</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/ShopTARge22.styles.css" asp-append-version="true" />
</head>
<body class="">
    <header id="header">
        <div class="inner">
            <a class="logo" asp-area="" asp-controller="Home" asp-action="Index">ShopTARge22</a>
            <nav id="nav">
                <a  asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                <a  asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
                <a  asp-area="" asp-controller="Spaceships" asp-action="Index">Spaceship</a>
                <a  asp-area="" asp-controller="Realestates" asp-action="Index">Realestates</a>
                <a  asp-area="" asp-controller="Kindergartens" asp-action="Index">Kindergartens</a>  
            </nav>
            <a href="#navPanel" class="navPanelToggle"><span class="fa fa-bars"></span></a>
        </div>
    </header>
    <main role="main" class="pb-3">
            @RenderBody()
    </main>
    <footer id="footer">
        <div class="inner">
            &copy; 2023 - ShopTARge22 - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <!-- <script src="~/lib/jquery/dist/jquery.min.js"></script> -->
    <!-- <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>-->
    <script src="~/js/jquery.min.js"></script>
    <script src="~/js/util.js"></script>
    <script src="~/js/skel.min.js"></script>
    <script src="~/js/main.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
